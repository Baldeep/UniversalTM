
public class NextState {

	private String q;
	private char output;
	private Direction next;
	
	public NextState(String q, char output, Direction next){
		this.q = q;
		this.output = output;
	}
	
	public String getState(){
		return q;
	}
	
	public char getOutput(){
		return output;
	}
	
	@Override
	public boolean equals(Object o){
		if( o instanceof State){
			return(this.q.equals(((State) o).getState()) && this.output == ((State) o).getInput());
		}
		return false;
	} 
	
	@Override public int hashCode() {
        return (41 * (41 + getState().length()) + 7 * getOutput());
    }
	
}

